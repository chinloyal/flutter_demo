    "version": "0.2.0",
    "configurations": [
        {
            "name": "flutter_demo main",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "flutter_demo staging",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define",
                "ENV=staging",
                "--flavor",
                "staging"
            ]
        },
        {
            "name": "flutter_demo prod",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define",
                "ENV=prod",
                "--flavor",
                "prod"
            ]
        },
        {
            "name": "flutter_demo profile",
            "request": "launch",
            "type": "dart",
            "args": [
                "--profile",
                "--trace-skia",
                "--dart-define",
                "ENV=prod",
                "--flavor",
                "prod"
            ]
        },
    ]
}